rpchart\\src\\App.js",[],"E:\\Pruebas\\bigcorpchart\\src\\Pages\\Layout\\Layout.jsx",[],"E:\\Pruebas\\bigcorpchart\\src\\Components\\Navbar\\NavBar.jsx",["64","65","66","67","68","69"],"E:\\Pruebas\\bigcorpchart\\src\\Pages\\TeamChart\\TeamChart.jsx",["70","71","72","73","74"],"import React, { useEffect, useState, useRef } from 'react'\r\nimport { useSelector } from 'react-redux'\r\nimport Levels from '../../Components/TeamChart/Levels'\r\nimport { getEmployeesByLimits , getEmployeesById } from '../../Services/ChartApi'\r\n\r\n\r\nexport default function TeamChart() {\r\n\r\n    const state = useSelector(state => state)\r\n    const [rowData, setRowData] = useState([])\r\n    const [dataExtra, setDataExtra] = useState([])\r\n\r\n\r\n    /**\r\n     * First render. I'm using this method because the all employees endpoint is not working.\r\n     * On that you request for all employees (because in that case , are just 20)\r\n     * and the manipulate the data to obtain the top level and then display the people who is below them,\r\n     * just the next level only\r\n     */\r\n\r\n    useEffect(() => {\r\n        let data\r\n        let lista = []\r\n        let managerId\r\n        getEmployeesByLimits(0,20).then(res => {\r\n            data = res.data\r\n        }).then(() => {\r\n            data.map(res => {\r\n                if (res.manager === 0) {\r\n                    lista.push(res)\r\n                    managerId = res.id\r\n                }\r\n               \r\n            })\r\n\r\n            let auxList = []\r\n            data.map(res => {\r\n                if (res.manager === managerId) {\r\n                    auxList.push(res)\r\n                }\r\n            })\r\n\r\n            let piramyd = [...lista, auxList]\r\n\r\n            setRowData(piramyd)\r\n\r\n        })\r\n\r\n\r\n\r\n\r\n\r\n    }, [])\r\n\r\n\r\n\r\n\r\n/**\r\n * This useeffect is fired when someone wants to see more in one resource, so this will search the subordinates for the id selected.\r\n */\r\n\r\nuseEffect(() => {\r\n\r\n    let id = state.display.display.managerId\r\n    if(state.display.display.status === true){\r\n        getEmployeesById(id).then(res => {\r\n            setDataExtra(res.data)\r\n        })\r\n    }\r\n\r\n\r\n}, [state.display.display])\r\n\r\n\r\n\r\n/**\r\n * This useEffect is used for manage the data when a user want to see more or less levels.\r\n */\r\n    useEffect(() => {\r\n\r\n\r\n        let data = [...rowData]\r\n        let cardSelected = state.display.display\r\n\r\n\r\n        if (cardSelected !== \"\") {\r\n\r\n            if (cardSelected.status) {\r\n                data = data.filter((res, index) => index <= cardSelected.row)\r\n                data = [...data, dataExtra]\r\n            } else {\r\n\r\n                if (cardSelected.row === 0) {\r\n                    data = data[0]\r\n                } else {\r\n                    data = data.filter((res, index) => index <= cardSelected.row)\r\n                }\r\n\r\n            }\r\n        }\r\n\r\n\r\n        setRowData(data)\r\n\r\n\r\n    }, [state.display.display, dataExtra])\r\n\r\n\r\n\r\n    /**\r\n     *This function display the data into the main page\r\n     */\r\n\r\n    const _showChart = () => {\r\n\r\n        let data = []\r\n\r\n        let list = [...rowData]\r\n\r\n        list.map((levels, index) => {\r\n            if (levels.length === undefined) {\r\n\r\n                data.push(<div className=\"row\" id={index} key={index}>\r\n                    <Levels employee={levels} id={index}></Levels>\r\n                </div>)\r\n            }\r\n            else {\r\n                let row = []\r\n\r\n                row.push(<div className=\"row\" id={index}>\r\n\r\n                    {levels.map((items, index) => {\r\n                        return <Levels employee={items} key={index} id={index}></Levels>\r\n                    })}\r\n                </div>)\r\n                data.push(row)\r\n\r\n\r\n            }\r\n\r\n        })\r\n\r\n\r\n\r\n        return data\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <div className=\"container-chart\">\r\n                {_showChart()}\r\n            </div>\r\n        </>\r\n    )\r\n}\r\n","E:\\Pruebas\\bigcorpchart\\src\\Components\\TeamChart\\Levels.jsx",["75","76"],"import React, { useEffect, useRef, useState } from 'react'\r\nimport './Levels.css'\r\nimport { boy, boy1, girl, girl1 } from './images'\r\nimport { displayRows, rowIndex } from '../../Redux/Actions/DisplayAction'\r\nimport { useDispatch, useSelector } from 'react-redux'\r\n\r\nexport default function Levels({ employee, id }) {\r\n\r\n\r\n    const [selected, setSelected] = useState(false)\r\n    const images = [boy, boy1, girl, girl1]\r\n    const indexRef = useRef()\r\n    const dispatch = useDispatch()\r\n    const state = useSelector(state => state)\r\n\r\n\r\n/**\r\n * this useEffect is used for fill the squares when it has been selected.\r\n */\r\n\r\n    useEffect(() => {\r\n        setSelected(false)\r\n        if (state.display.rowIndex.length > 0) {\r\n            state.display.rowIndex.map(res => {\r\n                if (res.row === indexRef.current.parentNode.id && res.col === indexRef.current.id) {\r\n                    if (res.status === true) {\r\n                        setSelected(true)\r\n                    } else if (res.status === false) {\r\n                        setSelected(false)\r\n                    }\r\n                }\r\n            })\r\n        }\r\n\r\n        \r\n    }, [state.display.rowIndex])\r\n\r\n\r\n    \r\n/**\r\n * \r\n *  This function is used to display the subordinates for the person selected. \r\n */\r\n    const _showMore = (e) => {\r\n        e.preventDefault()\r\n\r\n        let action = state.display.rowIndex.find(element => (element.row === indexRef.current.parentNode.id && element.col === indexRef.current.id))\r\n     \r\n        if (typeof action === \"object\") {\r\n\r\n            action = action.status\r\n        } else {\r\n            action = \"\"\r\n\r\n        }\r\n\r\n        setSelected(!selected)\r\n\r\n        if (action !== undefined) {\r\n            dispatch(rowIndex({ row: indexRef.current.parentNode.id, col: indexRef.current.id, status: !action }))\r\n            dispatch(displayRows({ row: indexRef.current.parentNode.id, col: indexRef.current.id, status: !action , managerId: employee.id}))\r\n        }\r\n    }\r\n\r\n\r\n\r\n    return (\r\n\r\n\r\n        <div className={`card ${selected ? \"selected\" : \"\"}`} ref={indexRef} id={id}>\r\n            <div className=\"card-container\">\r\n                <img src={images[Math.floor(Math.random() * (4 - 1) + 1)]} alt=\"avatar\"></img>\r\n                <span>{employee.first} {employee.last}</span>\r\n                <span><strong>Department: </strong>{employee.department}</span>\r\n                <span><strong>Office: </strong>{employee.office === null ? \"N/A\" : employee.office}</span>\r\n                <span><a href=\"#\" onClick={(e) => _showMore(e)}>{selected ? \"-\" : \"+\"}</a></span>\r\n            </div>\r\n        </div>\r\n\r\n\r\n    )\r\n}\r\n","E:\\Pruebas\\bigcorpchart\\src\\Components\\TeamChart\\images\\index.js",[],"E:\\Pruebas\\bigcorpchart\\src\\Redux\\Store.jsx",[],"E:\\Pruebas\\bigcorpchart\\src\\Redux\\Reducers\\index.jsx",[],"E:\\Pruebas\\bigcorpchart\\src\\Redux\\Reducers\\DisplayReducer.jsx",["77"],"import { rowIndex } from '../Actions/DisplayAction'\r\nimport { DISPLAYROWS , ROWINDEX} from '../Actions/Types'\r\n\r\nconst initialState = {display : \"\", rowIndex : []}\r\n\r\nexport const display = (state = initialState, action) => {\r\n    switch (action.type){\r\n        case DISPLAYROWS:\r\n            return {...state, display : action.payload}\r\n\r\n        case ROWINDEX:\r\n            let data = [...state.rowIndex]\r\n            data = data.filter((res, index) => index <= action.payload.row)\r\n            let len = data.length\r\n            let val = false\r\n\r\n            for (let i = 0; i < len; i++){\r\n                //console.log(typeof parseInt(data[i].row), data[i].row, \"payload\", typeof action.payload.row, action.payload.row)\r\n                if(data[i].row === action.payload.row){\r\n                    // console.log(\"entro acÃ¡\")\r\n                    data[i].col = action.payload.col\r\n                    data[i].status = action.payload.status\r\n                    val = true\r\n                    break\r\n                }\r\n\r\n               \r\n            }\r\n\r\n            if(!val){\r\n                data = [...state.rowIndex, action.payload]\r\n            }\r\n            return {...state, rowIndex : data}\r\n\r\n        default:\r\n            return state\r\n    }\r\n}","E:\\Pruebas\\bigcorpchart\\src\\Redux\\Actions\\Types.jsx",[],"E:\\Pruebas\\bigcorpchart\\src\\Redux\\Actions\\DisplayAction.jsx",[],"E:\\Pruebas\\bigcorpchart\\src\\Services\\ChartApi.jsx",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":30,"column":21,"nodeType":"84","endLine":30,"endColumn":33},{"ruleId":"82","severity":1,"message":"83","line":31,"column":21,"nodeType":"84","endLine":31,"endColumn":33},{"ruleId":"82","severity":1,"message":"83","line":32,"column":21,"nodeType":"84","endLine":32,"endColumn":33},{"ruleId":"82","severity":1,"message":"83","line":33,"column":21,"nodeType":"84","endLine":33,"endColumn":33},{"ruleId":"82","severity":1,"message":"83","line":34,"column":21,"nodeType":"84","endLine":34,"endColumn":33},{"ruleId":"82","severity":1,"message":"83","line":35,"column":21,"nodeType":"84","endLine":35,"endColumn":33},{"ruleId":"85","severity":1,"message":"86","line":1,"column":38,"nodeType":"87","messageId":"88","endLine":1,"endColumn":44},{"ruleId":"89","severity":1,"message":"90","line":28,"column":26,"nodeType":"91","messageId":"92","endLine":28,"endColumn":28},{"ruleId":"89","severity":1,"message":"90","line"